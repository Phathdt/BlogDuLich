<h2 class="page-header">Quản lý Tài khoản <%=link_to "Create New", new_admin_user_path, class:'btn btn-success pull-right' %></h2>
<% if @users.exists? %>
  <table class="table table-striped">
    <tr>
      <th>Name</th>
      <th>Email</th>
      <th>Ngày tạo</th>
      <th>Actions</th>
    </tr>
    <% @users.each do |user| %>
		<tr>
      		<td><%= user.name %></td>
      		<td><%= user.email %></td>
      		<td><%= user.created_at.to_time.strftime("%d - %m - %Y")  %>
      		<td><%=link_to "Edit", edit_admin_user_path(user), class:'btn btn-primary' %> </td>
    	</tr>
	<% end %>
  </table>
  <%= will_paginate @users %>
<% else %>
	<p>There are no users</p>
<% end %>
